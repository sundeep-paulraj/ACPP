$Out.= "<tr><td>".($i+1)."</td><td>".@FASTA_Header[$i+1]."</td><td>".$O_Loc_Score."</td><td>".$P_Loc_Score."</td><td>".$M_Loc_Score."</td><td>".$Final_Result_Out."</td><td>".$Dual_Score."</td><td>".$Pospo.'</tr>';
